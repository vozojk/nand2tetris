/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And (a=b, b=sel, out=and1);
    Not (in=sel, out=notS);
    And (a=notS, b=a, out=and2);
    Or (a=and1, b=and2, out=out);
}